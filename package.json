  "scripts": {
    "clean": "rm -rf public build",
    "lint": "eslint --rulesdir eslint-rules source config script *.js migrations",
    "watch:lint": "npm run lint -s && onchange 'source/**/*.js' 'config/*.js' 'migrations/*.js' '*.js' -v -- npm run lint -s",
    "test": "",
    "check": "npm run lint && npm test",
    "build:server": "webpack --colors --config webpack.config.server.js",
    "build:delayed-job": "webpack --colors --config webpack.config.delayed-job.js",
    "build:client:js": "webpack --colors --config webpack.config.browser.js",
    "build:client:css": "node-sass --output-style expanded -o public source/client/style/index.scss",
    "build": "npm run build:server && npm run build:delayed-job && npm run build:client:js && npm run build:client:css",
    "watch:build:server": "npm run build:server -- -w",
    "watch:build:delayed-job": "npm run build:delayed-job -- -w",
    "watch:build:client:js": "npm run build:client:js -- -w",
    "watch:build:client:css": "npm run build:client:css -- -w",
    "watch:build": "npm run watch:build:server & npm run watch:build:delayed-job & npm run watch:build:client:js & npm run watch:build:client:css",
    "dev": "node script/dev.js"
  }
