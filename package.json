  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.2",
    "babel-plugin-transform-class-properties": "6.5.2",
    "babel-plugin-transform-function-bind": "6.5.2",
    "babel-plugin-transform-react-constant-elements": "6.5.0",
    "babel-plugin-transform-react-inline-elements": "6.5.0",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-es2015-node5": "1.1.2",
    "babel-preset-react": "6.5.0",
    "browser-sync": "2.11.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "file-loader": "0.8.5",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "socket.io-client": "1.4.5",
    "webpack": "1.12.13"
  },
  "scripts": {
    "clean": "rm -rf public build",
    "lint": "eslint --rulesdir eslint-rules source config script *.js",
    "watch:lint": "nodemon --exec 'npm run lint --silent' -w source -w config -w script",
    "test": "",
    "check": "npm run lint && npm test",
    "build:server": "webpack --config webpack.config.server.js",
    "build:client:js": "webpack --config webpack.config.js",
    "build:client:css": "node-sass --output-style expanded -o public source/client/style/index.scss",
    "build": "npm run build:server && npm run build:client:js && npm run build:client:css",
    "watch:build:server": "npm run build:server -- -w",
    "watch:build:client:js": "npm run build:client:js -- -w",
    "watch:build:client:css": "npm run build:client:css -- -w",
    "predev": "npm run clean && mkdir public && mkdir build && npm run build:client:css",
    "dev:databases": "docker-compose up",
    "dev:server": "npm run watch:build:server & env NODE_ENV=development BLUEBIRD_WARNINGS=0 DEBUG='socket.io:*' nodemon -w build -w config -C build/server.js",
    "dev:client": "npm run watch:build:client:js & npm run watch:build:client:css",
    "dev:live-reload": "browser-sync start --config bs-config.js",
    "dev": "npm run dev:databases & npm run dev:server & npm run dev:client & npm run dev:live-reload"
  }
}
